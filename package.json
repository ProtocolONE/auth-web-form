  "scripts": {
    "serve:login": "vue-cli-service serve 'server/login.js'",
    "serve:registration:": "vue-cli-service serve 'server/registration.js'",
    "build": "yarn build:loader && yarn build:login --no-clean && yarn build:registration --no-clean",
    "build:loader": "vue-cli-service build --target wc --name auth-loader 'src/Loader.vue'",
    "build:login": "vue-cli-service build --target wc --name auth-login 'src/Login.vue'",
    "build:registration": "vue-cli-service build --target wc --name auth-registration 'src/Registration.vue'",
    "lint": "vue-cli-service lint",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit",
    "publish:npm": "yarn build:bundle && npm publish"
  },
  "dependencies": {
    "@protocol-one/auth-web-form": "^0.0.8",
    "@protocol-one/ui-kit": "^1.25.2",
    "axios": "^0.19.0",
    "btoa": "^1.2.1",
    "core-js": "^2.6.5",
    "events": "^3.0.0",
    "lodash-es": "^4.17.11",
    "vue": "^2.6.10",
    "vue-router": "^3.0.3",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.9.0",
    "@vue/cli-plugin-e2e-cypress": "^3.9.0",
    "@vue/cli-plugin-eslint": "^3.9.0",
    "@vue/cli-plugin-unit-jest": "^3.9.0",
    "@vue/cli-service": "^3.9.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "dotenv-webpack": "^1.7.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.12.0",
    "sass-loader": "^7.1.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue-template-compiler": "^2.6.10"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:ProtocolONE/auth-web-form.git"
  },
